{
  "compilerOptions": {
    "strictNullChecks": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "types": ["@cloudflare/workers-types", "node", "cypress"],
    "moduleResolution":"node",
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true
  },
  "include": [
    "**/*.ts*",
    "src",
    "./*.js",
    "declaration.d.ts",
    "./functions/*",
    "./lib/*"
  ]
}
