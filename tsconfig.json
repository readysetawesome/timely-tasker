{
  "compilerOptions": {
    "strictNullChecks": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "types": ["@cloudflare/workers-types", "@testing-library/jest-dom", "jest", "node"],
    "moduleResolution":"node",
    "jsx": "react",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./src/**/*",
    "declaration.d.ts",
    "./functions/*",
    "./lib/*"
  ]
}
